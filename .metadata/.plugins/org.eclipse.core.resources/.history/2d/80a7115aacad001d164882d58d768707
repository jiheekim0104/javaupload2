package javahighEx04;

import java.util.Iterator;
import java.util.TreeSet;

public class ComparableEx {
	
	public static void main(String[] args) {
		
		TreeSet<Person> treeSet = new TreeSet<Person>();
		
		treeSet.add(new Person("±èÁöÈñ", 30));
		treeSet.add(new Person("ÀÌÀçÈÆ", 34));
		treeSet.add(new Person("ÁøµµÁØ", 80));
		treeSet.add(new Person("Áø¾çÃ¶", 15));
		treeSet.add(new Person("Àå°¡À»", 22));
		treeSet.add(new Person("³²°æÁø", 43));
		
		Iterator<Person> iterator = treeSet.iterator();
		while(iterator.hasNext()) {
			Person member = iterator.next();
			System.out.println(member.getName()+" "+member.getAge());
		}
		System.out.println("-------------------");
		TreeSet<Person> newTreeSet = new TreeSet<Person>();
		newTreeSet.add(new Person("±èÁöÈñ", 30, 11.1));
		newTreeSet.add(new Person("ÀÌÀçÈÆ", 34, 21.8));
		newTreeSet.add(new Person("ÁøµµÁØ", 80, 33.1));
		newTreeSet.add(new Person("Áø¾çÃ¶", 15, 94.1));
		newTreeSet.add(new Person("Àå°¡À»", 22, 17.4));
		newTreeSet.add(new Person("³²°æÁø", 43, 83.1));
		
		iterator = newTreeSet.iterator();
		while(iterator.hasNext()) {
			Person member = iterator.next();
			System.out.println(member.getName()+" "+member.getAge());
		}
	}	
}
